<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.omniauthable? %>
  <div class="effect claudio">
    <%- resource_class.omniauth_providers.each do |provider| %>
      <div class="buttons">
        <% if provider.to_s == "facebook" %>
          <%= link_to omniauth_authorize_path(resource_name, provider), class: "fb" do %><br />
            <i class="fab fa-facebook"></i>
          <% end %>
        <% elsif provider.to_s == "github" %>
          <%= link_to omniauth_authorize_path(resource_name, provider), class: "github" do %><br />
<!--      <%#= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: ""%><br />-->
            <i class="fab fa-github"></i>
          <% end %>
        <% else %>
          <%= link_to omniauth_authorize_path(resource_name, provider), class: "g-plus" do %><br />
            <i class="fab fa-google"></i>
          <% end %>
        <% end %>
      </div>
    <% end %>
  </div>
<% end %>
